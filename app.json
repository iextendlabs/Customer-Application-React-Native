{
  "expo": {
    "name": "Lipslay",
    "slug": "Lipslay_App",
    "version": "1.4.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "buildNumber": "1.4",
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "Getting Location for Service Delivery"
      },
      "bundleIdentifier": "com.lipslay.Customerapp"
    },
    "android": {
      "versionCode": 9,
      "permissions": [
        "INTERNET",
        "ACCESS_WIFI_STATE",
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION"
      ],
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.lipslay.Customerapp"
    },
    "plugins": [
      [
        "expo-build-properties",
        {
          "ios": {
            "useFrameworks": "static"
          }
        }
      ]
    ],
    "web": {},
    "owner": "forexleo",
    "extra": {
      "eas": {
        "projectId": "441d9510-00b2-4425-9516-37a6c5dd3117"
      }
    }
  }
}
